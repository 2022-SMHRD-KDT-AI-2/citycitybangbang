<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="citycitybangbang.CitybangMapper">

   <select id="checkLogin" parameterType="citycitybangbang.model.MemberVO" resultType="citycitybangbang.model.MemberVO">
        select * from t_member
   		where mem_id=#{mem_id} and mem_pwd=#{mem_pwd} and mem_type='M'
   </select>
   
    <select id="reportList" resultType="citycitybangbang.model.ReportVO">
   		select * from t_report where re_date=#{date} 
   </select>
</mapper>